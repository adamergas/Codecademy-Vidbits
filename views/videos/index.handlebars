<div id="index-container">
  <div class="contents-container" id="videos-container">
    {{#each videos as |video|}}
    <div class="video-card">
      <div class="video-title">
        {{video.title}}
      </div>
      <div class="video-description">
        {{video.description}}
      </div>
    </div>
    {{/each}}
  </div>
  <a href="/videos/create">Create a Video</a>
</div>
